@article{hansen_optimal_2006,
	title = {Optimal Full Matching and Related Designs via Network Flows},
	volume = {15},
	issn = {1061-8600},
	url = {https://doi.org/10.1198/106186006X137047},
	doi = {10.1198/106186006X137047},
	abstract = {In the matched analysis of an observational study, confounding on covariates X is addressed by comparing members of a distinguished group (Z = 1) to controls (Z = 0) only when they belong to the same matched set. The better matchings, therefore, are those whose matched sets exhibit both dispersion in Z and uniformity in X. For dispersion in Z, pair matching is best, creating matched sets that are equally balanced between the groups; but actual data place limits, often severe limits, on matched pairs' uniformity in X. At the other extreme is full matching, the matched sets of which are as uniform in X as can be, while often so poorly dispersed in Z as to sacrifice efficiency.This article presents an algorithm for exploring the intermediate territory. Given requirements on matched sets' uniformity in X and dispersion in Z, the algorithm first decides the requirements' feasibility. In feasible cases, it furnishes a match that is optimal for X-uniformity among matches with Z-dispersion as stipulated. To illustrate, we describe the algorithm's use in a study comparing womens' to mens' working conditions; and we compare our method to a commonly used alternative, greedy matching, which is neither optimal nor as flexible but is algorithmically much simpler. The comparison finds meaningful advantages, in terms of both bias and efficiency, for our more studied approach.},
	pages = {609--627},
	number = {3},
	journaltitle = {Journal of Computational and Graphical Statistics},
	author = {Hansen, Ben B. and Klopfer, Stephanie Olsen},
	date = {2006-09-01},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1198/106186006X137047},
	keywords = {Matched sampling, Minimum cost flow, Nonrandom treatment assignment, Observational study, Quasiexperiment, Subclassification},
	file = {Full Text PDF:/Users/jackpotrykus/Zotero/storage/NBTFWIN6/Hansen and Klopfer - 2006 - Optimal Full Matching and Related Designs via Netw.pdf:application/pdf},
}

